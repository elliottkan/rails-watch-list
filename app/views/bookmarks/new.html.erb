<div class="container p-3">
  <h1>New Bookmark</h1>
  <%= simple_form_for([@list, @bookmark]) do |f| %>
    <%= f.input :comment %>
    <%= f.input :movie, collection: Movie.all, input_html: {multiple: true, data: {controller: "tom_select", tom_select_options_value: {tags: true}}}%>
    <%= f.button :submit, "Submit", class: "btn btn-dark w-100 mt-1" %>
  <% end %>
</div>
